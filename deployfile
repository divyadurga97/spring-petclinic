node{
stage('Download package from JFrog') {
            def server= Artifactory.server 'artifactory'
            def downloadSpec = """{
            "files": [
            {
                "pattern": "spring-petclinic/spring-petclinic-2.3.1.BUILD-SNAPSHOT.jar",
                "target": "./spring-petclinic.jar"
            }
            ]
            }"""
        server.download(downloadSpec)
    }
         stage('host') {
    sh label: '', script: 'java -jar target -httpPort=8080'
  }
}
